$('.qualification-item.<%= @id %>').hide().after('<%= j render "qualification_item_form" %>');
$('.edit, .add-item').hide()
$('.cancel-edit-qualification').click(function() {
  $('.qualification-item, .edit, .add-item').show()
  $('.edit-qualification-item').hide()
});
$('input:checkbox').change(
  function(){
    if ($('input:checkbox').is(':checked')){
      $(".end-date-wrap").hide()
    } else {
      $(".end-date-wrap").show()
    }
  });