$('.qualification-items').before('<%= j render "add_qualification_item" %>');
$('.edit, .add-item, .empty-section').hide()

$('input:checkbox').change(
  function(){
    if ($('input:checkbox').is(':checked')){
      $(".end-date-wrap").hide()
    } else {
      $(".end-date-wrap").show()
    }
  });

$('.cancel-edit-qualification').click(function() {
  $('.qualification-item, .edit, .add-item').show()
  $('.new-qualification-item').hide()
});